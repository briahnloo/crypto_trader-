# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit your actual .env file with real credentials!

# =============================================================================
# EXCHANGE API CREDENTIALS
# =============================================================================

# Coinbase Pro / Advanced Trade
# Get your API keys from: https://www.coinbase.com/settings/api
COINBASE_API_KEY=your_coinbase_api_key_here
COINBASE_SECRET=your_coinbase_secret_here
COINBASE_PASSPHRASE=your_coinbase_passphrase_here
COINBASE_SANDBOX=true

# Binance
# Get your API keys from: https://www.binance.com/en/my/settings/api-management
BINANCE_API_KEY=your_binance_api_key_here
BINANCE_SECRET=your_binance_secret_here
BINANCE_TESTNET=true

# =============================================================================
# DATA SOURCE API KEYS
# =============================================================================

# CoinGecko
COINGECKO_API_KEY=your_coingecko_api_key_here
COINGECKO_FREE_TIER=true

# Whale Alert
WHALE_ALERT_API_KEY=your_whale_alert_api_key_here

# Fear & Greed Index
FEAR_GREED_API_KEY=your_fear_greed_api_key_here

# Social Media APIs
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET=your_twitter_api_secret_here
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# PostgreSQL (optional)
DATABASE_URL=postgresql://user:password@localhost:5432/crypto_mvp

# =============================================================================
# TRADING CONFIGURATION
# =============================================================================

DEFAULT_SYMBOL=BTC/USDT
DEFAULT_TIMEFRAME=1h
LOG_LEVEL=INFO
ENVIRONMENT=development

# =============================================================================
# FEATURE FLAGS
# =============================================================================

REALIZATION_ENABLED=true
